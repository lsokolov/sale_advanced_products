<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_adv_product_order_wizard" model="ir.ui.view">
              <field name="name">Print advanced product order</field>
              <field name="model">adv.product.order.wizard</field>
              <field name="type">form</field>
              <field name="arch" type="xml">
                <data>
                    <group>
                    <field name="in"/>
                    <field name="out"/>
                    </group>
                    <footer>
                        <button name="order_print" type="object" string="Print" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </data>
              </field>
        </record>
        <act_window id="action_adv_product_order_wizard"
            name="Order print"
            res_model="adv.products.order.wizard"
            src_model="sale.advanced.products"
            view_mode="form"
                    view_id="view_adv_product_order_wizard"
        />
    </data>
</openerp>

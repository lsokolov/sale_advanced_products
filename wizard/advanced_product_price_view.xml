<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

    <!--   Product Price List -->

        <record id="view_product_price_list_extra" model="ir.ui.view">
              <field name="name">Price per unit extra</field>
              <field name="model">product.price_list</field>
              <field name="inherit_id" ref="product.view_product_price_list"/>
              <field name="arch" type="xml">
                <data>
                    <field name="qty4" position="replace"/>
                    <field name="qty5" position="replace"/>
                    <field name="qty3" position="after">
                        <field name="prn_img"/>
                    </field>
                </data>
              </field>
        </record>
    </data>
</openerp>
